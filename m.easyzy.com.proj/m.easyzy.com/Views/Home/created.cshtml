@using easyzy.model.dto;
@{var list = Model as List<dto_UserZy>;}
@{
    ViewBag.Title = "易作业移动端-已建作业";
}

<div class="content-wrap">
    <table id="tabques" border="0" cellspacing="1" cellpadding="0" class="tab-default">
        <thead>
            <tr>
                <th>作业编号</th>
                <th>创建时间</th>
                <th>答题卡</th>
                <th>答案/解析</th>
                <th>完成情况</th>
            </tr>
        </thead>
        <tbody id="tbody">
            @if (list != null)
            {
                foreach (dto_UserZy zy in list)
                {
                    <tr>
                        <td>@zy.ZyNum</td>
                        <td>@zy.CreateDate.ToString("yyyy-MM-dd HH:mm:ss")</td>
                        <td>@(zy.Structed ? "有" : "无")</td>
                        <td>@(zy.AnswerHtmlPath == "" ? "无" : "有")</td>
                        <td><a href="query?zynum=@zy.ZyNum">查看</a></td>
                    </tr>
                }
            }
            else
            {
                <tr><td colspan="5">尚未建过作业！</td></tr>
            }
        </tbody>
    </table>
</div>
<script type="text/javascript">
    $(".neckcontent .funcname").html("已建作业");
</script>