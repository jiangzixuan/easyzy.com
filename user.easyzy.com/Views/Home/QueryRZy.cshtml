@using easyzy.model.dto
@{var list = Model as List<dto_UserZy>;}

<div style="padding:5px 0;">
    @*<a href="javascript:void(0);" onclick="GetUserInfo();" title="返回"><img src="~/Images/arrow_left.png" width="40" height="30" /></a>*@
    @ViewBag.RTrueName 新建的作业：
</div>
<table id="tabques" border="0" cellspacing="1" cellpadding="0" class="tab-default">
    <thead>
        <tr>
            <th>作业编号</th>
            <th>创建时间</th>
            <th>完成情况</th>
            <th>做作业</th>
        </tr>
    </thead>
    <tbody id="tbody">
        @if (list != null)
        {
            foreach (dto_UserZy zy in list)
            {
                <tr>
                    <td>@zy.ZyNum</td>
                    <td>@zy.CreateDate.ToString("yyyy-MM-dd HH:mm:ss")</td>
                    <td><a href="http://easyzy.com/zy/query?zynum=@zy.ZyNum">查看</a></td>
                    <td><a href="http://easyzy.com/zy/open">做作业</a></td>
                </tr>
            }
        }
        else
        {
            <tr><td colspan="4">尚未建过作业！</td></tr>
        }
        <tr style="display:none"><td id="PageCountDiv" colspan="4">@ViewBag.PageCount</td></tr>
    </tbody>
</table>
<div id="pager" class="zypager"></div>